{
  "name": "ceres.consumer",
  "version": "1.0.14",
  "description": "Provider for network framework ceres. Used with ceres.protocol.",
  "main": "./dist/consumer.js",
  "types": "./dist/consumer.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json & ./node_modules/.bin/webpack",
    "build:watch": "./node_modules/.bin/webpack --watch",
    "build-ts": "tsc -p ./tsconfig.json",
    "build-ts:watch": "tsc -p ./tsconfig.json -w",
    "test:unit": "./node_modules/.bin/jasmine-ts",
    "tslint": "./node_modules/.bin/tslint -c tslint.json './src/**/*{.ts,.tsx}'",
    "tslint-fix": "concurrently \"./node_modules/.bin/tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'\""
  },
  "author": "Dmitry Astafyev (D.Astafyev@outlook.com)",
  "license": "Apache 2",
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "jasmine": "^3.3.0",
    "jasmine-core": "^3.3.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^7.0.1",
    "concurrently": "^4.1.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/DmitryAstafyev/ceres.git"
  },
  "keywords": [
    "network",
    "protocol",
    "JSON",
    "binary",
    "typescript",
    "encode",
    "decode",
    "converting",
    "validation",
    "server",
    "consumer",
    "http",
    "https",
    "ws",
    "wss"
  ]
}
