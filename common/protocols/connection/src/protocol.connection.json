{
    "Message": {
        "clientId"  : "string",
        "guid?"     : "guid",
        "Handshake": {
            "Response": {
                "token?"    : "string",
                "reason?"   : "Reasons",
                "error?"    : "string",
                "Reasons"   : ["FAIL_AUTH", "NO_TOKEN_FOUND", "NO_CLIENT_ID_FOUND"]
            }
        },
        "Reconnection": {
            "Request" : {
                "token": "string"
            },
            "Response": {
                "allowed": "boolean"
            },
            "Responses": ["ConnectionError"]
        },
        "Hook": {
            "Request" : {
                "token": "string"
            },
            "Responses": ["ConnectionError", "Disconnect"]
        },
        "Pending": {
            "Request" : {
                "token": "string"
            },
            "Response": {
                "event?": "EventDefinition",
                "demand?": "DemandDefinition",
                "return?": "DemandDefinition"
            },
            "Responses": ["Disconnect"]
        },
        "Event": {
            "Request": {
                "event": "EventDefinition",
                "token": "string",
                "aliases?": "array<KeyValue>"
            },
            "Response": {
                "subscribers": "integer"
            },
            "Responses" : ["ConnectionError"]
        },
        "Subscribe": {
            "Request": {
                "subscription" : "Subscription",
                "token": "string"
            },
            "Response": {
                "status": "boolean",
                "error?": "string"
            },
            "Responses" : ["ConnectionError"]
        },
        "Unsubscribe": {
            "Request": {
                "subscription": "Subscription",
                "token": "string"
            },
            "Response": {
                "status": "boolean",
                "error?": "string"
            },
            "Responses" : ["ConnectionError"]
        },
        "UnsubscribeAll": {
            "Request": {
                "subscription": "Subscription",
                "token": "string"
            },
            "Response": {
                "status": "boolean",
                "error?": "string"
            },
            "Responses" : ["ConnectionError"]
        },
        "Registration": {
            "Request": {
                "aliases": "array<KeyValue>",
                "token": "string"
            },
            "Response": {
                "status": "boolean",
                "error?": "string"
            },
            "Responses" : ["ConnectionError"]
        },
        "Demand": {
            "FromExpectant": {
                "Request": {
                    "demand": "DemandDefinition",
                    "token": "string",
                    "query": "array<KeyValue>",
                    "options?": "Options"
                },
                "Response": {
                    "id": "string",
                    "state": "State",
                    "error?": "string"
                },
                "Responses" : ["ConnectionError"]
            },
            "FromRespondent": {
                "Request": {
                    "id": "string",
                    "token": "string",
                    "error?": "string",
                    "demand?": "DemandDefinition"
                },
                "Response": {
                    "status": "boolean",
                    "error?": "string"
                },
                "Responses" : ["ConnectionError"]
            },
            "State": ["ERROR", "NO_RESPONDENTS", "SUCCESS", "DEMAND_SENT", "PENDING"],
            "@Options": {
                "scope?": "Scope",
                "Scope": ["local", "hosts", "clients", "all"]
            }
        },
        "Respondent": {
            "Bind" : {
                "Request": {
                    "demand": "string",
                    "protocol": "string",
                    "token": "string",
                    "query": "array<KeyValue>"
                },
                "Response": {
                    "status": "boolean",
                    "error?": "string"
                },
                "Responses" : ["ConnectionError"]
            },
            "Unbind": {
                "Request": {
                    "demand": "string",
                    "protocol": "string",
                    "token": "string"
                },
                "Response": {
                    "status": "boolean"
                },
                "Responses" : ["ConnectionError"]
            }
        }
    },
    "EventDefinition": {
        "protocol": "string",
        "event": "string",
        "body": "string"
    },
    "DemandDefinition": {
        "id": "string",
        "protocol": "string",
        "demand": "string",
        "body": "string",
        "expected": "string",
        "error?": "string",
        "pending?": "boolean"
    },
    "Subscription": {
        "protocol": "string",
        "event?": "string"
    },
    "ConnectionError": {
        "reason": "Reasons",
        "message": "string",
        "Reasons": ["FAIL_AUTH", "NO_TOKEN_FOUND", "NO_CLIENT_ID_FOUND", "NO_TOKEN_PROVIDED", "TOKEN_IS_WRONG", "UNEXPECTED_REQUEST", "NO_DATA_PROVIDED"]
    },
    "Disconnect": {
        "reason": "Reasons",
        "message?": "string",
        "Reasons": ["SHUTDOWN"]
    },
    "KeyValue": {
        "key": "string",
        "value": "string"
    },
    "version": "0.0.1"
}